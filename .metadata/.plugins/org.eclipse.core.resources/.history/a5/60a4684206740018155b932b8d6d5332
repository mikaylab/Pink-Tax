/*
 * Item represents a product from the store. Each item has a price, description, brand, type, name, and ID number.  
 *
 */
public class Item {
	private double price;
	private String description;
	private Brand brand;
	private int id;
	private String name;
	private String type;
	
	public Item(double price, Brand brand, int id, String name, String type) {
		this.price = price;
		this.brand = brand;
		this.id = id;
		this.type = type;
	}
}
