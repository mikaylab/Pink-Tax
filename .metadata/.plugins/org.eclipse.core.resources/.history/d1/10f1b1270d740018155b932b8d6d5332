/*
 * Defines all the types of products in the store. 
 */
public enum Type {
	//PRODUCT TYPES
	TOYS("Toys"),
	CLOTHING("Clothing"),
	FEM_HYGIENE("Feminine Hygiene"),
	
	HBA("Health & Beauty");
	
	private String name;
	private Type(String name) {
		this.name = name;
	}
	public String toString() { return name; }
	
}
